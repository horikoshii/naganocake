<h2>商品一覧</h2>

 <thead>
    <tr>
     <th>商品id</th>
     <th>商品名</th>
     <th>税抜価格</th>
     <th>ジャンル</th>
     <th>ステータス</th>
    </tr>
 </thead>
 <tbody>
    <% @item.each do |item| %>

    <tr>
     <td><%= item.id %></td>
     <td><%= item.name %></td>
     <td><%= item.price %></td>
    </tr>
    <% end %>
  </tbody>