<div class="container">
  <div class="row">
    <h2>会員情報編集</h2>
  </div>
  <%= form_with model: @customer, url:customer_customer_path(:id),method: :PATCH do |f| %>
  <div class="row">
    <div class="col-lg-4">氏名</div>
    <div class="col-lg-4"><%= f.text_field :last_name %></div>
    <div class="col-lg-4"><%= f.text_field :first_name %></div>
  </div>
  <% end %>
</div>