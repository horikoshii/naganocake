<h2>登録</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>


  <div class="field">
    <%= f.label :名前 %>
    <p>（姓）<%= f.text_field :last_name %></p>
    <p>（名）<%= f.text_field :first_name %></p>
  </div>

  <div class="field">
    <%= f.label :フリガナ %>
    <p>（セイ）<%= f.text_field :last_name_kana %></p>
    <p>（メイ）<%= f.text_field :first_name_kana %></p>
  </div>

  <div class="field">
    <%= f.label :メールアドレス %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :郵便番号（ハイフンなし） %>
    <%= f.text_field :postal_code %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "customer/shared/links" %>
